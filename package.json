{
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "laravel-up": "./vendor/bin/sail up -d",
        "laravel-down": "./vendor/bin/sail down",
        "tinker": "./vendor/bin/sail tinker",
